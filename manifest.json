{
  "name": "Histree",
  "version": "1.5",
  "manifest_version": 2,
  "icons": {
  	"16": "treerings16.png",
    "48": "treerings48.png",
    "128": "treerings128.png" },
  "description": "Histree",
  "background": {
    "scripts": ["models.js","storage.js","urlcontrol.js","bg.js"],
    "persistent": false
    },
  "permissions": [
    "<all_urls>",
    "tabs",
    "storage",
    "unlimitedStorage"
  ],
  "chrome_url_overrides" : {
    "history": "histree.html"
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}