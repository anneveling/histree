{
  "name": "histree",
  "version": "1.2",
  "manifest_version": 2,
  "description": "Histree",
  "background": {
    "scripts": ["models.js","storage.js","urlcontrol.js","bg.js"],
    "persistent": false
    },
  "permissions": [
    "<all_urls>",
    "tabs",
    "storage",
    "unlimitedStorage"
  ],
  "chrome_url_overrides" : {
    "history": "histree.html"
  }
}