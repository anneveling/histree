{
  "name": "histree",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Hello to yesterday",
  "background": {
    "scripts": ["bg.js"],
    "persistent": false
    },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "<all_urls>",
    "tabs",
    "storage",
    "unlimitedStorage"
  ],
  "chrome_url_overrides" : {
    "history": "histree.html"
  }
}